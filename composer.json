{
    "name": "thomasvinh-san-byte/ag-vote",
    "description": "Application de gestion de votes et assemblées générales",
    "license": "proprietary",
    "require": {
        "php": ">=8.4",
        "dompdf/dompdf": "^3.1",
        "phpoffice/phpspreadsheet": "^1.29",
        "erusev/parsedown": "^1.8",
        "symfony/mailer": "^8.0",
        "symfony/console": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^10.5"
    },
    "config": {
        "platform": {
            "php": "8.4.0"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "AgVote\\": "app/",
            "AgVote\\Command\\": "app/Command/",
            "AgVote\\Controller\\": "app/Controller/",
            "AgVote\\Core\\": "app/Core/",
            "AgVote\\Core\\Security\\": "app/Core/Security/",
            "AgVote\\Core\\Validation\\": "app/Core/Validation/",
            "AgVote\\Event\\": "app/Event/",
            "AgVote\\Repository\\": "app/Repository/",
            "AgVote\\Service\\": "app/Services/",
            "AgVote\\View\\": "app/View/",
            "AgVote\\WebSocket\\": "app/WebSocket/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AgVote\\Tests\\": "tests/"
        }
    }
}
